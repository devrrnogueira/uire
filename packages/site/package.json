{
    "name": "site",
    "version": "0.1.0",
    "dependencies": {
        "css-transition-rule": "0.0.2",
        "highlight.js": "^10.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3"
    },
    "scripts": {
        "start": "craco start",
        "build-old": "react-scripts build",
        "build": "PUBLIC_URL=/uire react-scripts build && rm -rf docs/ && mv build/ docs/ && cp 404.html docs/404.html",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "indent": "off",
            "eqeqeq": "off",
            "prefer-const": "off",
            "space-before-function-paren": "off",
            "no-throw-literal": "off",
            "import/no-webpack-loader-syntax": "off"
        }
    },
    "browserslist": {
        "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
        ],
        "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@craco/craco": "^6.1.1",
        "raw-loader": "^4.0.2"
    }
}
